        <File Permission="644" Location="Kernel/System/CMDBExplorer.pm"></File>
        <File Permission="644" Location="Kernel/System/CMDBExplorer/GraphVizRenderer.pm"></File>
        <File Permission="644" Location="Kernel/System/CMDBExplorer/ObjectWrapper.pm"></File>
        <File Permission="644" Location="Kernel/System/CMDBExplorer/Scope.pm"></File>
        <File Permission="644" Location="Custom/Kernel/Modules/AgentITSMConfigItemZoom.pm"></File>
        <File Permission="644" Location="Custom/Kernel/Modules/AgentITSMServiceZoom.pm"></File>
